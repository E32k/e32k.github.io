---
layout: none
---
[
  {% for page in site.pages %}
    {% if page.url startswith '/beamng/' %}
      {
        "title"    : "{{ page.title | escape }}",
        "category" : "{{ page.category }}",
        "tags"     : "{{ page.tags | join: ', ' }}",
        "url"      : "{{ page.baseurl }}{{ page.url }}",
        "date"     : "{{ page.date }}"
      }{% unless forloop.last %},{% endunless %}
    {% endif %}
  {% endfor %}
]
