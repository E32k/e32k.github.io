---
layout: none
---
[
  {% for page in site.pages %}
    {
      "title"    : "{{ page.title | escape }}",
      "category" : "{{ page.category }}",
      "content"  : "{{ page.content | strip_html | strip_newlines | remove_chars | escape }}"
      "url"      : "{{ page.baseurl }}{{ page.url }}",
      "date"     : "{{ page.date }}"
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
